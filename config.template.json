{
  "openai_api_key": "<PUT_YOUR_OPENAI_API_KEY_HERE>",
  "ollama": {
    "enabled": true,
    "cmd": "ollama",
    "path": "C:\\Users\\sbatt\\AppData\\Local\\Programs\\Ollama\\ollama.exe",
    "default_model": "llama2",
    "api_url": "http://localhost:11434"
  },
  "python_path": "<PATH_TO_PYTHON_EXE_OR_EMPTY>",
  "docker_enabled": false,
  "local_llm_path": "<PATH_TO_LOCAL_MODEL_OR_BINARY>",
  "agent_secrets": {
    "custom_agent_key": "<PUT_CUSTOM_AGENT_KEY_HERE>"
  },
  "user_notes": "replace-with-personal-settings (do NOT commit)"
  ,
  "agent_defaults": {
    "vramGB": 2,
    "memoryGB": 4,
    "cpus": 2
  }
  ,
  "skills_schema": {
    "description": "Per-agent skill tags used for capability matching and composite agent composition.",
    "examples": ["nlp:code-review", "nlp:unit-test-gen", "vision:ocr", "ops:monitoring", "analysis:data-summary"],
    "note": "Skills are free-form strings namespaced with a category (category:capability). Use '-' for multi-word. Keep short tags."
  },
  "monitor": {
    "MemoryPressureThresholdGB": 6,
    "CpuPressurePercent": 80,
    "RollingWindow": 60
  }
}
